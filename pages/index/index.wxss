page {
  background-color: #336699;
  font-family: "黑体","宋体";
}
.title {
  color: white;
  font-size: 20px;
  text-align: center;
  line-height: 30px;
  background-color: orange;
  box-shadow: 2px 2px 10px #888;
  margin-bottom: 10px;
}
.desc {
  color: white;
}
.big-circle {
  width: 150px;
  height: 150px;
  position: absolute;
  border-radius: 50%;
}
.time {
  height: 160px;
}
.right {
  clip: rect(0,auto,auto,75px)
}
.blue {
  background-color: #99ccff;
}
.blue1 {
  background-color: #09f;
}
.btnGroud {
  display: flex;
  flex-direction: row;
}
.btn{
  background-color: #09c;
  color: white;
}
.white {
  background-color: #fff;
}
.circle {
   width: 130px;
   height: 130px;
   border-radius: 50%;
   position: absolute;
   margin-top: 10px;
   margin-left: 10px;
}
.circle view{
  margin-left: 50px;
